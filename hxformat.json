{
	"emptyLines": {
		"classEmptyLines": {
			"existingBetweenFields": "keep",
			"afterVars": 0,
			"afterPrivateVars": 0,
			"afterStaticVars": 2
		},
		"afterFieldsWithDocComments": "ignore",
		"beforeDocCommentEmptyLines": "ignore",
		"maxAnywhereInFile": 4,
		"afterBlocks": "keep"
	},
	"sameLine": {
		"anonFunctionBody": "keep",
		"functionBody": "keep",
		"caseBody": "keep",
		"catchBody": "keep",
		"comprehensionFor": "keep",
		"doWhile": "keep",
		"doWhileBody": "keep",
		"elseBody": "keep",
		"elseIf": "keep",
		"expressionCase": "keep",
		"expressionIf": "keep",
		"expressionTry": "keep",
		"expressionIfWithBlocks": false,
		"forBody": "keep",
		"ifBody": "keep",
		"ifElse": "keep",
		"returnBody": "keep",
		"returnBodySingleLine": "keep",
		"tryBody": "keep",
		"tryCatch": "keep",
		"untypedBody": "keep",
		"whileBody": "keep"
	},
	"whitespace": {
		"typeHintColonPolicy": "around"
	},
	"wrapping": {
		"functionSignature": {
			"defaultWrap": "keep"
		},
		"opAddSubChain": {
			"defaultWrap": "fillLine"
		},
		"multiVar": {
			"defaultWrap": "fillLine"
		},
		"callParameter": {
			"defaultWrap": "keep",
			"rules": [
				{
					"conditions": [
						{
							"cond": "hasMultilineItems",
							"value": 1
						}
					],
					"type": "keep"
				}
			]
		},
		"anonType": {
			"defaultWrap": "keep"
		},
		"objectLiteral": {
			"defaultWrap": "keep"
		}
	}
}