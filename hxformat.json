{
	"emptyLines": {
		"classEmptyLines": {
			"existingBetweenFields": "keep",
			"afterVars": 0,
			"afterPrivateVars": 0,
			"afterStaticVars": 2
		},
		"afterFieldsWithDocComments": "ignore",
		"beforeDocCommentEmptyLines": "ignore",
		"maxAnywhereInFile": 4
	},
	"sameLine": {
		"anonFunctionBody": "keep",
		"functionBody": "keep",
		"expressionIf": "keep",
		"ifBody": "keep"
	},
	"whitespace": {
		"typeHintColonPolicy": "around"
	},
	"wrapping": {
		"callParameter": {
			"defaultWrap": "keep"
		}
	}
}